ACB.Name = "ACB"
ACB.Parent = Items
ACB.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
ACB.Position = UDim2.new(0.108870968, 0, 0.048971191, 0)
ACB.Size = UDim2.new(0, 200, 0, 50)
ACB.Font = Enum.Font.SourceSansBold
ACB.Text = "AntiCheat Bypass"
ACB.TextColor3 = Color3.fromRGB(255, 255, 255)
ACB.TextScaled = true
ACB.TextSize = 14.000
ACB.TextWrapped = true
ACB.MouseButton1Down:Connect(function()
	local a = getrawmetatable(game)
	local sucks = a.__namecall
	local player = game.Players.LocalPlayer
	setreadonly(a, false)
	a.__namecall = newcclosure(function(name, ...)
		local tabs = {
			...
		}
		if getnamecallmethod() == "FireServer" and tostring(name) == "MainEvent" then
			if tabs[1] == "CHECKER_1" or tabs[1] == "TeleportDetect" or tabs[1] == "OneMoreTime" then
				return wait(9e9)
			end
		end
		if getnamecallmethod() == "Kick" then
			return wait(9e9)
		end
		return sucks(name, unpack(tabs))
	end)
	setreadonly(a, true)
end)

